  "bugs": {
    "url": "https://github.com/TopherBeauchamp/RecordStore/issues"
  },
  "homepage": "https://github.com/TopherBeauchamp/RecordStore#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "cookie-parser": "^1.4.7",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.6.0",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "dotenv": "^16.0.3",
    "nodemon": "^3.1.4"
  }
